{
  "env": {
    "browser": true,
    "es6": true,
    "jquery": true,
    "node": true
  },
  "globals": {
    "Handlebars": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "constructor-super": "warn",
    "max-len": ["warn", 120],
    "no-alert": "warn",
    "no-console": "warn",
    "no-const-assign": "warn",
    "no-debugger": "warn",
    "no-this-before-super": "warn",
    "no-undef": "warn",
    "no-unreachable": "warn",
    "no-unused-vars": "warn",
    "semi": ["warn", "always"],
    "valid-typeof": "warn"
  }
}
